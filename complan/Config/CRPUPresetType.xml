<PresetType>
  <Id>TypeCRPU</Id>
  <UriTemplate>http://{host-address}/restconf/data/crpu-conf</UriTemplate>
  <Description>Предустановка CRPU</Description>
  <AllowedHosts><HostType>crpu</HostType></AllowedHosts>
  <PresetList>
    <Preset id="CRPU_dummy">
    </Preset>
    <Preset id="CRPU__preset_250300Hz">
		<crpu-conf>
		  <index_work_channel>{index}</index_work_channel>
		  <ch>
			<index>{index}</index>
			<recv_frq>250300</recv_frq>
			<transv_frq>250300</transv_frq>
			<connector>Default</connector>
			<noise_gate>On</noise_gate>
		  </ch>
		</crpu-conf>
    </Preset>
    <Preset id="CRPU_preset_110KHz">
		<crpu-conf>
		  <index_work_channel>{index}</index_work_channel>
		  <ch>
			<index>{index}</index>
			<recv_frq>110000</recv_frq>
			<transv_frq>110000</transv_frq>
			<connector>Default</connector>
			<noise_gate>On</noise_gate>
		  </ch>
		</crpu-conf>
    </Preset>
    <Preset id="CRPU_preset_140KHz">
		<crpu-conf>
		  <index_work_channel>{index}</index_work_channel>
		  <ch>
			<index>{index}</index>
			<recv_frq>140000</recv_frq>
			<transv_frq>140000</transv_frq>
			<connector>Default</connector>
			<noise_gate>On</noise_gate>
		  </ch>
		</crpu-conf>
    </Preset>
  </PresetList>
</PresetType>