<PresetType>
  <Id>TypeN1000</Id>
  <UriTemplate>http://{host-address}/restconf/data/n1000-conf</UriTemplate>
  <Description>Предустановка N1000</Description>
  <AllowedHosts><HostType>n1000</HostType></AllowedHosts>
  <PresetList>
    <Preset id="N1000_dummy">
    </Preset>
    <Preset id="N1000_preset_250300Hz">
		<n1000-conf>
		  <index_work_channel>{index}</index_work_channel>
		  <ch>
			<index>{index}</index>
			<recv_frq>250300</recv_frq>
			<transv_frq>250300</transv_frq>
			<connector>Default</connector>
			<noise_gate>On</noise_gate>
		  </ch>
		</n1000-conf>
    </Preset>
    <Preset id="N1000_preset_110KHz">
		<n1000-conf>
		  <index_work_channel>{index}</index_work_channel>
		  <ch>
			<index>{index}</index>
			<recv_frq>110000</recv_frq>
			<transv_frq>110000</transv_frq>
			<connector>Default</connector>
			<noise_gate>On</noise_gate>
		  </ch>
		</n1000-conf>
    </Preset>
    <Preset id="N1000_preset_140KHz">
		<n1000-conf>
		  <index_work_channel>{index}</index_work_channel>
		  <ch>
			<index>{index}</index>
			<recv_frq>140000</recv_frq>
			<transv_frq>140000</transv_frq>
			<connector>Default</connector>
			<noise_gate>On</noise_gate>
		  </ch>
		</n1000-conf>
    </Preset>
  </PresetList>
</PresetType>